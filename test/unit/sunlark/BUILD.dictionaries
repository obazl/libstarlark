package(default_visibility = ["//visibility:public"])

load("@rules_cc//cc:defs.bzl", "cc_binary", "cc_library", "cc_test")

dict_library(
    name = "stringdict",

    adict = {"akey1": "aval1", akey2: "aval2", "akey3": "aval3"},
    bdict = {
        "bkey1": "bval1", "bkey2": "bval2", "bkey3": "bval3"
    },
    cdict = {
        "ckey1": "cval1",
        "ckey2": "cval2",
        "ckey3": "cval3"
    }
)

dict_test(
    name = "stringlist-dict",

    adict = {
        "akey1": ["astr11", "astr12", "astr13"],
        "akey2": ["astr21", "astr22", "astr23"],
        "akey3": ["astr31", "astr32", "astr33"]
    },

    bdict = {
        "bkey1": ["bstr11", "bstr12", "bstr13"],
        "bkey2": ["bstr21", "bstr22", "bstr23"],
        "bkey3": ["bstr31", "bstr32", "bstr33"]
    },
)

## comment line
dict_test(
    name = "intdict",
    adict = {1: 10, 2: 20, 3: 30},
    avec  = [11, 12, 13],
    bdict = {
        10: "a", 20: "b", 30: "c"
    },
    cdict = {
        "a": 100,
        "b": 200,
        "c": 300
    }
)
