@REGEN_BENCHMARKS_TRUE@	$(AM_V_at)mkdir -p $(@D)
@REGEN_BENCHMARKS_TRUE@	$(AM_V_GEN)$(RAGEL) -G2 $< -o $@
@REGEN_BENCHMARKS_TRUE@	$(AM_V_at)if ! cmp -s $@ $(PREGEN_DIR)/ragel/$(@F) ; then cp -f $@ $(PREGEN_DIR)/ragel/$(@F) ; fi

@REGEN_BENCHMARKS_TRUE@$(GEN_KLEENEX): $(GEN_DIR)/%.c: $(SRC_DIR)/%.kex $(KLEENEX)
@REGEN_BENCHMARKS_TRUE@	$(AM_V_at)mkdir -p $(@D)
@REGEN_BENCHMARKS_TRUE@	$(AM_V_GEN)$(KLEENEX) compile $< --srcout $@ --act=false --la=true
@REGEN_BENCHMARKS_TRUE@	$(AM_V_at)if ! cmp -s $@ $(PREGEN_DIR)/kleenex/$(@F) ; then cp -f $@ $(PREGEN_DIR)/kleenex/$(@F) ; fi

@REGEN_BENCHMARKS_FALSE@$(GEN_RAGEL) $(GEN_KLEENEX): $(GEN_DIR)/%: $(PREGEN_DIR)/%
@REGEN_BENCHMARKS_FALSE@	$(AM_V_at)mkdir -p $(@D)
@REGEN_BENCHMARKS_FALSE@	$(AM_V_GEN)cp -f $< $@

$(BIN_GCC): $(BIN_DIR)/%-gcc: $(GEN_DIR)/%.c
	$(AM_V_at)mkdir -p $(@D)
	$(AM_V_at)gcc $(CFLAGS) -c -o $@.o $<
	$(AM_V_GEN)gcc $(CFLAGS) -o $@ $@.o

$(BIN_CLANG): $(BIN_DIR)/%-clang: $(GEN_DIR)/%.c
	$(AM_V_at)mkdir -p $(@D)
	$(AM_V_at)clang $(CFLAGS) -c -o $@.o $<
	$(AM_V_GEN)clang $(CFLAGS) -o $@ $@.o

$(DAT): $(DAT_DIR)/%/big: $(DAT_DIR)/%/small
	$(AM_V_at)mkdir -p $(@D) && cp $(srcdir)/$(@D)/small $(@D)
	$(AM_V_GEN)( cd $(@D) && ../../$(srcdir)/data/gen.py )

$(KLEENEX):
	$(AM_V_GEN)( cd $(@D) && ./getkleenex.sh )

$(RAGEL):
	$(AM_V_at)mkdir -p $(@D) && cp $(srcdir)/engines/ragel/getragel7.sh $(@D)
	$(AM_V_GEN)( cd $(@D) && ./getragel7.sh )

clean-local:
	$(AM_V_at)rm -f $(GEN) $(OBJ) $(BIN) $(DAT)

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
